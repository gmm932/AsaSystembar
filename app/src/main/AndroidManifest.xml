<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gmm.asasystembar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DemoActivity" android:screenOrientation="portrait" />
        <activity android:name=".AsaSystemBarTestActivity" android:screenOrientation="portrait"/>
        <activity android:name=".SimpleTintBarActivity" android:screenOrientation="portrait"/>
        <activity android:name=".SlideViewTintActivity" android:screenOrientation="portrait"></activity>
    </application>

</manifest>